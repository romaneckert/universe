{
    "name": "public",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build": "npm run build-de-en && npm run build-de-de && app/index.js",
        "build-de-de": "ng build --prod --build-optimizer --aot --output-path public/de/de --base-href /de/de/ --locale de --i18n-format xlf --i18n-file web/locale/messages.de.xlf --missing-translation error ",
        "build-de-en": "ng build --prod --build-optimizer --aot --output-path public/de/en --base-href /de/en/ --locale en ",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "i18n": "ng xi18n --outputPath web/locale --outFile messages.xlf"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "5.2.10",
        "@angular/common": "5.2.10",
        "@angular/compiler": "5.2.10",
        "@angular/core": "5.2.10",
        "@angular/http": "5.2.10",
        "@angular/platform-browser": "5.2.10",
        "@angular/platform-browser-dynamic": "5.2.10",
        "@angular/router": "5.2.10",
        "@angular/forms": "5.2.10",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.10",
        "zone.js": "^0.8.26",
        "bcrypt": "^2.0.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "cookie-parser": "^1.4.3",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "helmet": "^3.12.0",
        "jsonwebtoken": "^8.2.0",
        "mongoose": "^5.0.13",
        "pug": "^2.0.3",
        "winston": "^3.0.0-rc4"
    },
    "devDependencies": {
        "@angular/cli": "1.7.4",
        "@angular/compiler-cli": "5.2.10",
        "@angular/language-service": "5.2.10",
        "del": "^3.0.0",
        "gulp": "4.0",
        "gulp-clean": "^0.4.0",
        "gulp-pug": "^4.0.1",
        "typescript": "2.6.2"
    }
}
